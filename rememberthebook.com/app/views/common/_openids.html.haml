- User.omniauth_providers.each do |provider|
  - if provider == 'vkontakte_open_api'
    = vkontakte_login_button.html_safe
  - else
    = link_to image_tag("signin/#{provider}_16.png", :size => "16x16", :alt => provider), (provider==:google ? '/users/auth/google' : user_omniauth_authorize_path( provider )), :class=>provider
