<h1>Login</h1>
<% form_for @user_session, :url => user_session_path do |f| %>
	<%= f.error_messages %>
	<p class="email">
		<%= f.label :email %><br />
		<%= f.text_field :email %>
	</p>
	<p class="password">
		<%= f.label :password %><br />
		<%= f.password_field :password %>
	</p>
	<p class="remember_me">
		<%= f.check_box :remember_me %><%= f.label :remember_me %>
	</p>
	<p class="sumbit">
		<%= f.submit "Login" %>
	</p>
<% end %>